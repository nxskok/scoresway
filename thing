<script type="text/javascript">
  $$('div.pitch_position').each(function(el) {
    el.observe('mouseover', function() {
      el.down('.player_popup').show();
      el.toggleClassName('popup_active');
    });
    el.observe('mouseout', function() {
      el.down('.player_popup').hide();
      el.toggleClassName('popup_active');
    });
  });

// prevent popups from appearing outside field
document.observe("dom:loaded", function()
{
  var pops = $$('.pitch_position .player_popup');
  pops.each(function(pop) {
    var area = pop.up('.pitch_view_container');
    var area_offset_left = area.cumulativeOffset();
    var parent = pop.up('.pitch_position');
    var parent_left_offset = parent.positionedOffset()['left'];
    var popup_width = pop.getWidth();
    var popup_height = pop.getHeight();
    var shirt_number = parent.down('.shirtnumber');
    var field_width = $('field').getWidth();

    if( (parent_left_offset + popup_width) > field_width) {
      pop.addClassName('_left');
    }

    var parent_top_offset = parent.positionedOffset()['top'];

    if($(parent).hasClassName('sub')) {
        pop.style.bottom = (popup_height + shirt_number.getWidth())+'px';
        pop.style.right = (popup_width - (shirt_number.getWidth()/2) )+'px';
    }

    if( (parent_top_offset - popup_height) < -15) {
      pop.addClassName('_top');
    }
  });

  // shorten names
  $$('.pitch_position > .playername').each(function(el) {
    var playername = el.innerHTML;

    if( playername.substring(1,3) == '. ') {
      el.innerHTML = playername.substring(3);
    }
  });
});
</script><div class="lineups">
